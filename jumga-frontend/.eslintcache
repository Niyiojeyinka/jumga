 {\n          if (actionAdd) {\n            addToWishlist(product);\n            if (auth.loggedIn) {\n              setActionAdd(false);\n            }\n          } else {\n            removeFromWishlist(product, products);\n          }\n        }}\n        className=\"btn color-yellow text-white\"\n      >\n        {actionAdd ? \"Add to Wishlist\" : \"Remove from Wishlist\"}\n      </button>\n    );\n  } else {\n    return <Redirect to={redirectTo} />;\n  }\n};\n\nexport default WishlistToggle;\n","/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/helpers/removefromproducts.js",["340"],"/** help remove a product from products array\n *\n * @param {*} productID id of product to remove\n * @param {*} products  products array\n */\nconst removeFromProducts = (productID, products) => {\n  return products?.filter((product, index) => {\n    return product?.id != productID;\n  });\n};\n\nexport default removeFromProducts;\n","/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/components/scrolltotop.js",["341"],"import React, { useEffect, Fragment } from \"react\";\nimport { withRouter } from \"react-router-dom\";\n\nfunction ScrollToTop({ history, children }) {\n  useEffect(() => {\n    const unlisten = history.listen(() => {\n      window.scrollTo(0, 0);\n    });\n    return () => {\n      unlisten();\n    };\n  }, []);\n\n  return <Fragment>{children}</Fragment>;\n}\n\nexport default withRouter(ScrollToTop);\n",{"ruleId":"342","replacedBy":"343"},{"ruleId":"344","replacedBy":"345"},{"ruleId":"346","severity":1,"message":"347","line":28,"column":11,"nodeType":"348","endLine":28,"endColumn":14},{"ruleId":"346","severity":1,"message":"347","line":31,"column":11,"nodeType":"348","endLine":31,"endColumn":14},{"ruleId":"349","severity":1,"message":"350","line":8,"column":42,"nodeType":"351","messageId":"352","endLine":8,"endColumn":44},{"ruleId":"353","severity":1,"message":"354","line":5,"column":8,"nodeType":"355","messageId":"356","endLine":5,"endColu