ck = () => {\n    setToggleSidebar(!toggleSidebar);\n  };\n  return (\n    <>\n      <div className=\"wrapper\">\n        <nav id=\"sidebar\" className={`${toggleSidebar ? \"active\" : \"\"}`}>\n          <div className=\"sidebar-header\">\n            <h3>JUMGA</h3>\n          </div>\n\n          <ul className=\"list-unstyled components\">\n            <div className=\"p-3\">\n              <b>John Doe</b>\n              <br></br>\n\n              <small>Dispatch Rider</small>\n            </div>\n            <li>\n              <a href=\"#\">Manage Orders</a>\n            </li>\n            <li>\n              <a href=\"#\">Withdrawal</a>\n            </li>\n\n            <li>\n              <a href=\"#\">Payment Settings</a>\n            </li>\n            <li>\n              <a href=\"#\">Settings</a>\n            </li>\n          </ul>\n\n          <ul className=\"list-unstyled CTAs\">\n            <li>\n              <a className=\"article\">Sign Out</a>\n            </li>\n          </ul>\n        </nav>\n\n        <div id=\"content\">\n          <nav className=\"navbar navbar-expand-lg navbar-light bg-light\">\n            <div className=\"container-fluid\">\n              <button\n                onClick={handleClick}\n                type=\"button\"\n                id=\"sidebarCollapse\"\n                className={`navbar-btn ${toggleSidebar ? \"active\" : \"\"}`}\n              >\n                <span></span>\n                <span></span>\n                <span></span>\n              </button>\n            </div>\n          </nav>\n          {props.children}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default MerchantTemplate;\n","/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/helpers/request.js",["441","442"],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/reducers/systemvar.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/actions/actionTypes.js",[],"/home/olaniyi/Desktop/challenge/ecomme