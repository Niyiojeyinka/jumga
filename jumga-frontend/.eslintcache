te(<></>);\n  const { id } = useParams();\n  const [quantity, setQuantity] = useState(0);\n\n  useEffect(async () => {\n    const response = await request(\"products/\" + id, \"GET\");\n    const prdt = response.body.data?.product;\n    setImageToShow(\n      prdt.images[0]?.slug\n        ? BACKEND_BASE_URL + \"\" + prdt.images[0]?.slug\n        : defaultImage\n    );\n    setProduct(prdt);\n    if (prdt.images[0]?.slug) {\n      const holdImagesJSX = prdt.images.map((productImg, index) => {\n        console.log(\"hey\");\n        return (\n          <img\n            onClick={() => {\n              setImageToShow(BACKEND_BASE_URL + \"\" + productImg.slug);\n            }}\n            src={BACKEND_BASE_URL + \"\" + productImg.slug}\n            className=\"border m-1\"\n            key={\"im\" + index}\n          />\n        );\n      });\n      setImagesJSX(holdImagesJSX);\n    }\n  }, []);\n\n  return (\n    <>\n      <Header>\n        <div className=\"\">\n          <Breadcrumb category={\"computers\"} name={product?.name} />\n          <br></br>\n          <strong className=\"product-name\">{product?.name}</strong>\n        </div>\n        <div className=\"row\">\n          <div className=\"col-sm-12 col-md-9 col-lg-9 col-xl-9\">\n            <img src={imageToShow} width=\"80%\" />\n          </div>\n          <div className=\"col-sm-12 col-md-3 col-lg-3 col-xl-3 feature-image-nav\">\n            {imagesJSX}\n            <div className=\"m-2\">\n              <b>${product.price}</b>\n            </div>\n            <div>\n              <NumberInput\n                max={product.in_stock}\n                getValue={(value) => {\n                  setQuantity(value);\n                }}\n              />\n            </div>\n            <div>\n              <CartToggle quantity={quantity} product={product} />\n\n              <WishlistToggle product={product} />\n            </div>\n          </div>\n        </div>\n      </Header>\n      <section className=\"row\">\n        <div className=\"col-sm-12 col-md-3 col-lg-3 col-xl-3 p-5 predefined-desc\">\n          <div>\n            <strong>SKU</strong> :\n            <i>{product.sku != null ? product.sku : \"---\"}</i>\n          </div>\n          <div>\n            <strong>Model</strong> :\n            <i>{product.model != null ? product.model : \"---\"}</i>\n          </div>\n          <div>\n            <strong>Color</strong> :\n            <i>{product.color != null ? product.color : \"---\"}</i>\n          </div>\n          <div>\n            <strong>Weight</strong> :\n            <i>{product.weight != null ? product.weight : \"---\"}</i>\n          </div>\n        </div>\n        <div className=\"col-sm-12 col-md-9 col-lg-9 col-xl-9 py-1 px-5 product-description\">\n          <b className=\"desc\">Description</b>\n          <br></br>\n          <article>\n            <div\n              dangerouslySetInnerHTML={{\n                __html: product?.description,\n              }}\n            />\n          </article>\n        </div>\n      </section>\n      <section className=\"p-5\">\n        <ProductList\n          products={products.random}\n          showAddToWishlistBtn={true}\n          title=\"Top Products\"\n        />\n      </section>\n\n      <Footer />\n    </>\n  );\n};\n\nexport default ProductPage;\n","/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/reducers/index.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/reportWebVitals.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/divisions/productslist.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/divisions/reviews.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/divisions/benefits.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/divisions/footer.js",[],"/home/olaniyi/Desktop/challenge/ecommerce/jumga-frontend/src/components/breadcrumb.js",["403","404","405","406"],"const Breadcrumb = (props) => {\n